{
    "name": "vimcord",
    "version": "1.0.41",
    "description": "A powerful Discord.js wrapper.",
    "license": "MIT",
    "author": "xsqu1znt",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "default": "./dist/index.js"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "build": "tsup",
        "check": "tsc --noEmit",
        "format": "prettier --write \"./**/*.{ts,json}\""
    },
    "dependencies": {
        "chalk": "^4.1.2",
        "discord.js": "^14.25.1",
        "dotenv": "^16.6.1",
        "qznt": "^1.0.36"
    },
    "devDependencies": {
        "@types/node": "^22.19.11",
        "mongoose": "^8.23.0",
        "prettier": "^3.8.1",
        "tsup": "^8.5.1",
        "typescript": "^5.9.3"
    },
    "peerDependencies": {
        "discord.js": "^14.0.0",
        "mongoose": ">=8.0.0"
    },
    "peerDependenciesMeta": {
        "discord.js": {
            "optional": false
        },
        "mongoose": {
            "optional": true
        }
    }
}
